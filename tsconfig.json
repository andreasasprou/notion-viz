{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "baseUrl": "./",
    "sourceRoot": "/",
    "sourceMap": true,
    "inlineSources": true,
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "tsBuildInfoFile": ".tsbuildinfo",
    "paths": {
      "db": ["db/index.ts"],
      "@/graph/*": ["app/graph/*"],
      "@/ui/*": ["app/ui/*"],
      "@/constants/*": ["utils/constants/*"],
      "@/logger": ["utils/logger/index.ts"],
      "@/hooks/*": ["utils/hooks/*"],
      "@/shared": ["utils/shared/index.ts"],
      "@/types": ["utils/types/index.ts"],
      "@/utils/*": ["utils/*"]
    }
  },
  "exclude": ["node_modules", "**/*.e2e.ts", "cypress"],
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"]
}
